<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.temple.bitcoinapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".BitcoinActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NOTUSED_BitcoinDetailActivity"
            android:label="@string/title_bitcoin_detail"
            android:parentActivityName=".BitcoinActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.temple.bitcoinapp.BitcoinActivity" />
        </activity>
    </application>

</manifest>